{# Awards & Accreditations - Variant 1 #}
<div class="bg-white py-8">
  <div class="container">
    <div class="flex flex-wrap md:-mx-4">
      {% for item in accreditations %}
        <div class="w-full md:w-1/3 lg:w-1/5 md:px-4 mb-4 lg:mb-0">
          <div class="bg-primary text-center py-5">
            <span class="text-white">{{ item.image.title}}</span>
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</div>