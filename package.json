{
  "name": "adtrak-components-fractal",
  "version": "0.1.0",
  "dependencies": {
    "@frctl/fractal": "^1.3.0",
    "@frctl/twig": "^1.2.0",
    "tailwindcss": "^1.7.3"
  },
  "devDependencies": {
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.5"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "concurrently \"fractal start --sync\" \"npm run development -- --watch\"",
    "prod": "npm run production",
    "production": "concurrently \"fractal build\" \"cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js\""
  }
}
